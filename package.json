{
  "name": "vue-spa-ts-template",
  "version": "0.0.3",
  "description": "TypeScript шаблон на webpack 4 для Vue SPA",
  "scripts": {
    "serve": "cross-env NODE_ENV=development webpack --config build/configs/webpack.development.config.ts",
    "build": "cross-env NODE_ENV=production TS_NODE_PROJECT=\"build/tsconfig.json\" webpack --config build/configs/webpack.production.config.ts",
    "test": "cross-env NODE_ENV=test webpack --config build/configs/webpack.test.config.ts"
  },
  "repository": "https://github.com/newbornfrontender/vue-spa-ts-template.git",
  "author": "Infant Frontender <newbornfrontender@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/dotenv": "^4.0.3",
    "@types/mini-css-extract-plugin": "^0.2.0",
    "@types/node": "^10.5.3",
    "@types/webpack": "^4.4.8",
    "@types/webpack-merge": "^4.1.3",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "dotenv": "^6.0.0",
    "mini-css-extract-plugin": "^0.4.1",
    "ts-loader": "^4.4.2",
    "ts-node": "^7.0.0",
    "tsconfig-paths": "^3.5.0",
    "typescript": "^3.1.0-dev.20180727",
    "webpack": "^4.16.3",
    "webpack-cli": "^3.1.0",
    "webpack-merge": "^4.1.3"
  }
}
